#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/bundles/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/bundles/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/compiler-cli/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules/@angular/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/@angular+compiler-cli@17.3.9_@angular+compiler@17.3.9_typescript@5.3.3/node_modules:/home/day08/Documentos/DAY08/DEV/ANGULAR/gt_academy_employees/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular/compiler-cli/bundles/ngcc/index.js" "$@"
else
  exec node  "$basedir/../@angular/compiler-cli/bundles/ngcc/index.js" "$@"
fi
